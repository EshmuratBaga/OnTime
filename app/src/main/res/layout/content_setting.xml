<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="project.ontime.kz.ontime.screen.setting.SettingActivity"
    tools:showIn="@layout/activity_setting">

    <ImageButton
        android:id="@+id/iBtnCube"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_cube"
        android:layout_margin="10dp"/>

    <ImageButton
        android:id="@+id/iBtnOcta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_octa"
        android:layout_margin="10dp"/>

    <ImageButton
        android:id="@+id/iBtnDode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_dode"
        android:layout_margin="10dp"/>


</LinearLayout>
